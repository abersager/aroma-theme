{
  "name": "aroma-theme",
  "displayName": "Aroma theme",
  "description": "macOS goes Nord",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.96.0"
  },
  "publisher": "abersager",
  "repository": {
    "type": "git",
    "url": "https://github.com/abersager/aroma-theme"
  },
  "icon": "images/aroma-logo.png",
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Aroma",
        "uiTheme": "vs-dark",
        "path": "./vscode/Aroma-color-theme.json"
      }
    ]
  },
  "scripts": {
    "build": "mkdir -p .build && vsce package --out .build && cursor --install-extension $(find .build -type f -name \"*.vsix\" -print | sort -r | head -n 1)"
  },
  "galleryBanner": {
    "color": "#181818",
    "theme": "dark"
  },
  "badges": [
    {
      "url": "https://img.shields.io/github/stars/abersager/aroma-theme?style=social",
      "href": "https://github.com/abersager/aroma-theme",
      "description": "Star this theme on GitHub"
    }
  ],
  "markdown": "github"
}
